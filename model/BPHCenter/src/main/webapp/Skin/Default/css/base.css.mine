.ty-screen-mode i:active{background:url("../images/buttons.png") no-repeat -249px -712px;}
.ty-screen-mode-btn1{top:13px;left:20px;}
.ty-screen-mode-btn2{top:13px;left:56px;}
.ty-screen-mode-btn3{top:13px;left:92px;}
.ty-input{float:left;width:auto;height:auto;list-style-type: none;}
.ty-input-end{float:left;width:10px;height:26px;background:url("../images/icon.png") no-repeat -188px 0;position:absolute;}
.ty-input:hover .ty-input-end{background-position:-398px 0px;}
.ty-textarea{background-color:#040607;border:1px solid #597684;resize:none;}
.ty-total-decorate{width:200px;height:38px;position:absolute;top:-54px;right:200px;overflow:hidden;}
