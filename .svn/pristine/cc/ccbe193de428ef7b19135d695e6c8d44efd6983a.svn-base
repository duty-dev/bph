package com.tianyi.bph.dao.base;

import java.util.List;

import com.tianyi.bph.dao.MyBatisRepository;
import com.tianyi.bph.domain.base.Vehicle;
import com.tianyi.bph.query.base.VehicleQuery;
@MyBatisRepository
public interface VehicleDAO {
	
    int deleteByPrimaryKey(Integer id);

    int insert(Vehicle vehicle);


    Vehicle selectByPrimaryKey(Integer id);


    int updateByPrimaryKey(Vehicle vehicle);
    
    
    //分页查询
    List<Vehicle> findByPage(VehicleQuery vehicleQuery);
    //分页总条数
    int findCount(VehicleQuery vehicleQuery);
    //条件查询
    List<Vehicle> findByQuery(VehicleQuery vehicleQuery);
    
}