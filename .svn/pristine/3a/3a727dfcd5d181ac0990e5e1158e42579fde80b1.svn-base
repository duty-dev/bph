    </set>
    where ID = #{id,jdbcType=DECIMAL}
  </update>
  
  <!-- 通过id该sortNo -->
 <update id="changeSortNo" parameterType="com.tianyi.bph.domain.base.Police">
    update POLICE_USER
  <set>
     <if test="sortNo != null">
        SORT_NO = #{sortNo,jdbcType=DECIMAL},
      </if>
    </set>
    where ID = #{id,jdbcType=DECIMAL}
 </update>
 
 <!-- 查询最大的sortNo -->
 <select id="selectMaxSortNo" resultType="java.lang.Integer">
    select decode(MAX(SORT_NO),null,0,MAX(SORT_NO))  from POLICE_USER
 </select>
 
 	<!-- 条件、分页 -->
 	<sql id="find">
		select <include refid="Base_Column_List" />, <include refid="Organ_Column_List" />
		,<include refid="ManType_Column_List" />,<include refid="Duty_Type_Column_List"/>
    	from POLICE_USER P 
    	inner join Organ O on P.ORGAN_ID = O.ID 
    	left join MAN_TYPE M on P.MAN_TYPE_CODE = M.CODE
    	left join Police_Duty_Type PD on P.ID = PD.ID 
    	left join Duty_Type DT on PD.DUTY_CODE = DT.CODE 
		where 1=1 
		<if test="path != null and path != '' and !child">
			AND O.PATH  =  #{path,jdbcType=VARCHAR}
		</if>
		<if test="path != null and path != '' and child">
			AND O.PATH  like #{path,jdbcType=VARCHAR}||'%' 
		</if>
		<if test="name != null and name != ''">
			AND P.NAME  like '%'||#{name,jdbcType=VARCHAR}||'%' 
		</if>
		 order by P.ORGAN_ID, P.SORT_NO
	</sql>
	
	
	
	<select id="findByPage" parameterType="com.tianyi.bph.query.base.PoliceQuery"  resultMap="AllResultMap" >
		    
		    <!-- select * from ( select r.*, rownum ro from (
			<include refid="find" />
			) r  where rownum &lt;=#{end}) where ro &gt;#{begin} -->
			
			
			select <include refid="Base_Column_List" />, <include refid="Organ_Column_List" />,
			<include refid="ManType_Column_List" />,<include refid="Duty_Type_Column_List" /> ,
			<include refid="GpsDevice_Column_List" /> ,<include refid="Intercom_Column_List" /> 
			from 
			(select * from (select P.*, rownum ro from (select distinct <include refid="Base_Column_List" /> from (<include refid="find" /> ) P ) P
			where rownum &lt;=#{end}) where ro &gt;#{begin}) P 
			left join Organ O on P.ORGAN_ID = O.ID 
    	   left join MAN_TYPE M on P.MAN_TYPE_CODE = M.CODE
    	   left join Police_Duty_Type PD on P.ID = PD.ID 
    	   left join Duty_Type DT on PD.DUTY_CODE = DT.CODE
    	   left join GPS_DEVICE G on P.GPS_DEVICE_ID = G.ID 
    	   left join INTERCOM I on P.INTERCOM_ID = I.ID
	</select>
	
	<select id="findCount"  resultType="Integer">
		select count(distinct ID) from (
			<include refid="find" />
		) count_
	</select>
	
	<select id="findByQuery" parameterType="com.tianyi.bph.query.base.PoliceQuery"  resultMap="AllResultMap" >
		<include refid="find" />
	</select>
</mapper>