  <insert id="insert" parameterType="com.tianyi.bph.domain.base.Police">
    <selectKey statementType="STATEMENT" order="BEFORE" resultType="Integer" keyProperty="id" >  
			SELECT POLICE_USER_SEQ.NEXTVAL FROM DUAL
	</selectKey>
    insert into POLICE_USER (ID, CREATE_TIME, STATE, 
      GPS_DEVICE_ID, IDENTITY_CARD, INTERCOM_ID, 
      NAME, QUICK_CODE, NOTE, 
      PHONE, POLICE_ID, POSITION, 
      SEX, SHORT_CODE, UPDATE_TIME, 
      ORGAN_ID, MAN_TYPE_CODE, POLICE_TYPE_CODE, 
      ETHNIC, ADDRESS, EMAIL, 
      IS_PARTY, MARK_STATUS, SORT_NO
      )
    values (#{id,jdbcType=DECIMAL}, #{createTime,jdbcType=TIMESTAMP}, #{state,jdbcType=DECIMAL}, 
      #{gpsDeviceId,jdbcType=DECIMAL}, #{identityCard,jdbcType=VARCHAR}, #{intercomId,jdbcType=DECIMAL}, 
      #{name,jdbcType=VARCHAR}, #{quickCode,jdbcType=VARCHAR}, #{note,jdbcType=VARCHAR}, 
      #{phone,jdbcType=VARCHAR}, #{policeId,jdbcType=VARCHAR}, #{position,jdbcType=VARCHAR}, 
      #{sex,jdbcType=DECIMAL}, #{shortCode,jdbcType=VARCHAR}, #{updateTime,jdbcType=TIMESTAMP}, 
      #{organId,jdbcType=DECIMAL}, #{manTypeCode,jdbcType=VARCHAR}, #{policeTypeCode,jdbcType=VARCHAR}, 
      #{ethnic,jdbcType=VARCHAR}, #{address,jdbcType=VARCHAR}, #{email,jdbcType=VARCHAR}, 
      #{isParty,jdbcType=DECIMAL}, #{markStatus,jdbcType=DECIMAL}, #{sortNo,jdbcType=DECIMAL}
      )
  </insert>
  
  <update id="update" parameterType="com.tianyi.bph.domain.base.Police">
    update POLICE_USER
    <set>
      <if test="createTime != null">
