package com.tianyi.bph.query.system;

import com.tianyi.bph.query.BaseQuery;

public class CardPointQuery extends BaseQuery{

	private String name;
	private Integer organOrNext;
	private Integer organId;
	private String code;
	private Integer id;
	private String params;
	private String path;

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

	public String getParams() {
		return params;
	}

	public void setParams(String params) {
		this.params = params;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	public Integer getOrganId() {
		return organId;
	}

	public void setOrganId(Integer organId) {
		this.organId = organId;
	}

	public Integer getOrganOrNext() {
		return organOrNext;
	}

	public void setOrganOrNext(Integer organOrNext) {
		this.organOrNext = organOrNext;
	}
	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

}
